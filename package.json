{
  "name": "nodepress",
  "version": "6.1.0",
  "description": "RESTful API service for Surmon.me blog",
  "author": "Surmon",
  "license": "MIT",
  "homepage": "https://github.com/surmon-china/nodepress#readme",
  "documentation": "https://github.surmon.me/nodepress",
  "repository": {
    "type": "git",
    "url": "https://github.com/surmon-china/nodepress.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "doc": "compodoc -p tsconfig.compodoc.json -n 'NodePress documentation' -d documentation --disableCoverage --hideGenerator --customFavicon branding/favicon.ico",
    "release": ". ./scripts/release-via-github.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.x",
    "@fastify/cookie": "^11.x",
    "@fastify/multipart": "^9.x",
    "@nestjs/axios": "^4.0.x",
    "@nestjs/common": "^11.1.x",
    "@nestjs/core": "^11.1.x",
    "@nestjs/event-emitter": "^3.x",
    "@nestjs/jwt": "^11.x",
    "@nestjs/mapped-types": "^2.1.x",
    "@nestjs/platform-fastify": "^11.x",
    "@nestjs/throttler": "^6.5.x",
    "@redis/client": "^5.x",
    "@typegoose/auto-increment": "^5.x",
    "@typegoose/typegoose": "^13.x",
    "akismet-api": "^6.x",
    "axios": "^1.13.x",
    "bcryptjs": "^3.0.x",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.x",
    "cross-env": "^10.x",
    "dayjs": "^1.11.x",
    "fast-xml-parser": "^5.3.x",
    "googleapis": "^170.x",
    "js-base64": "^3.7.x",
    "jsonwebtoken": "^9.0.x",
    "lodash": "^4.x",
    "mongoose": "~9.1.4",
    "node-schedule": "^2.1.x",
    "nodemailer": "^7.x",
    "picocolors": "^1.x",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.x",
    "rxjs": "^7.8.x",
    "shelljs": "^0.10.x",
    "ua-parser-js": "^2.x",
    "yargs-parser": "^22.x"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.x",
    "@eslint/js": "^9.x",
    "@nestjs/cli": "^11.0.x",
    "@nestjs/schematics": "^11.x",
    "@nestjs/testing": "^11.x",
    "@types/jest": "30.x",
    "@types/jsonwebtoken": "^9.x",
    "@types/lodash": "^4.17.x",
    "@types/node": "^25.x",
    "@types/node-schedule": "^2.x",
    "@types/nodemailer": "^7.x",
    "@types/shelljs": "^0.10.x",
    "@types/supertest": "^6.x",
    "@types/validator": "^13.15.x",
    "eslint": "^9.x",
    "eslint-config-prettier": "^10.x",
    "eslint-plugin-prettier": "^5.x",
    "fastify": "5.7.x",
    "globals": "^17.x",
    "jest": "^30.x",
    "prettier": "^3.8.x",
    "supertest": "^7.x",
    "ts-jest": "^29.x",
    "ts-loader": "^9.5.x",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.x",
    "typescript-eslint": "^8.x"
  },
  "engines": {
    "node": ">=22"
  }
}
