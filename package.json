    "helmet": "^3.4.1",
    "hiredis": "^0.5.0",
    "jsonwebtoken": "^7.2.1",
    "marked": "^0.3.6",
    "mongoose": "^4.8.4",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-paginate": "^5.0.0",
    "nodemailer": "^3.1.4",
    "nodemailer-smtp-transport": "^2.7.2",
    "qn": "^1.3.0",
    "redis": "^2.7.1",
    "request": "^2.79.0",
    "simple-netease-cloud-music": "^0.2.0",
    "sitemap": "^1.9.0",
