{
  "name": "nodepress",
  "version": "3.11.0",
  "description": "RESTful API service for Surmon.me blog",
  "author": "Surmon",
  "license": "MIT",
  "homepage": "https://github.com/surmon-china/nodepress#readme",
  "documentation": "https://github.surmon.me/nodepress",
  "repository": {
    "type": "git",
    "url": "https://github.com/surmon-china/nodepress.git"
  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "doc": "compodoc -p tsconfig.json -n 'NodePress documentation' -d documentation --disableCoverage",
    "release": ". ./scripts/release.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.170.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/throttler": "^3.0.0",
    "@typegoose/auto-increment": "^1.7.0",
    "@typegoose/typegoose": "^9.12.0",
    "akismet-api": "^5.3.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cache-manager": "^4.1.0",
    "chalk": "4.x",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "fast-xml-parser": "^4.0.10",
    "googleapis": "^107.0.0",
    "helmet": "^6.0.0",
    "js-base64": "^3.7.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "~6.6.0",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.8",
    "passport": "0.5.0",
    "passport-jwt": "^4.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "shelljs": "^0.8.5",
    "ua-parser-js": "^1.0.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "29.0.0",
    "@types/lodash": "^4.14.185",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.36",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.0",
    "jest": "^29.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
